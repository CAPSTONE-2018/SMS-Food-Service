<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/BusinessRegistration"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jaren.foodservice_mobileapp.RegisterBusinessActivity">

    <EditText
        android:id="@+id/etBusinessUsername"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessPassword" />

    <TextView
        android:id="@+id/tvBusinessUsername"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="Username"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessPassword"
        app:layout_constraintEnd_toStartOf="@+id/etBusinessPassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/etBusinessPassword"
        android:layout_width="176dp"
        android:layout_height="27dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessUsername"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvBusinessPassword"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="32dp"
        android:text="Password"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessEmail"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvBusinessEmail"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="32dp"
        android:text="Email"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessAddress"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etBusinessEmail"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessEmail" />

    <TextView
        android:id="@+id/tvBusinessPhone"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="148dp"
        android:layout_marginStart="32dp"
        android:text="Business Phone"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etBusinessPhone"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="148dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessPhone" />

    <TextView
        android:id="@+id/tvBusinessAddress2"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="32dp"
        android:text="Zipcode"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessName"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etBusinessZip"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessAddress2" />

    <TextView
        android:id="@+id/tvBusinessAddress"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="32dp"
        android:text="Address"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessAddress2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etBusinessAddress"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessZip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessAddress" />

    <TextView
        android:id="@+id/tvBusinessRegistration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Business Registration"
        android:textSize="24dp"
        app:layout_constraintBottom_toTopOf="@+id/btnBusinessRegister"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/tvBusinessName"
        android:layout_width="146dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginStart="32dp"
        android:text="Business Name"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvBusinessPhone"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/etBusinessName"
        android:layout_width="176dp"
        android:layout_height="31dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/etBusinessPhone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/tvBusinessName" />

    <Button
        android:id="@+id/btnBusinessRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="20dp" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />
</android.support.constraint.ConstraintLayout>
